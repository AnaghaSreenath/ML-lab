        "# ---------------------------\n",
        "#   LAB: LINEAR, RIDGE, LASSO\n",
        "# ---------------------------\n",
        "\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from sklearn.linear_model import LinearRegression, Ridge, Lasso\n",
        "from sklearn.metrics import mean_squared_error\n",
        "\n",
        "# -----------------------------------------------------\n",
        "# 1. SAMPLE DATA (Replace this with your class dataset)\n",
        "# -----------------------------------------------------\n",
        "# Example dataset: simple linear relationship\n",
        "X = np.array([1, 2, 3, 4, 5]).reshape(-1, 1)   # Feature\n",
        "Y = np.array([1.2, 1.9, 3.2, 3.9, 5.1])        # Actual target values\n",
        "\n",
        "df = pd.DataFrame({\"X\": X.flatten(), \"Y\": Y})\n",
        "df\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# -------------------------------\n",
        "# 2. LINEAR REGRESSION\n",
        "# -------------------------------\n",
        "lin = LinearRegression()\n",
        "lin.fit(X, Y)\n",
        "\n",
        "print(\"Linear Regression:\")\n",
        "print(\"Coefficient (m):\", lin.coef_[0])\n",
        "print(\"Intercept (b):\", lin.intercept_)\n",
        "\n",
        "# Predict values\n",
        "Y_pred_lin = lin.predict(X)\n",
        "\n",
        "print(\"\\nPredicted Values (Linear):\", Y_pred_lin)\n",
        "print(\"MSE (Linear):\", mean_squared_error(Y, Y_pred_lin))\n"
      ],
